{
  "name": "react-codegen-cli",
  "version": "1.2.0",
  "description": "Software Development Kit for React",
  "main": "lib/index.js",
  "bin": {
    "react-codegen": "lib/bin.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "commit": "cz",
    "test": "nyc ts-mocha --config .mocharc.js --paths -p ./tsconfig.json ./src/tests/unit/**/*.test.ts",
    "build": "rollup -c",
    "watch": "yarn build -w",
    "lint": "eslint src --ext .ts",
    "start": "node ./lib/bin.js",
    "release": "semantic-release",
    "release:env": "export $(cat .env | xargs) && semantic-release"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "src/tests"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "json"
    ],
    "all": true
  },
  "dependencies": {
    "@babel/generator": "^7.11.0",
    "@babel/parser": "^7.11.3",
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0",
    "@sentry/node": "^6.0.1",
    "@sentry/tracing": "^6.0.1",
    "chalk": "^4.1.0",
    "console-clear": "^1.1.1",
    "cosmiconfig": "^6.0.0",
    "detect-indent": "^6.0.0",
    "find-up": "^4.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.19",
    "log-symbols": "^4.0.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/babel__generator": "^7.6.1",
    "@types/babel-generator": "^6.25.3",
    "@types/babel-traverse": "^6.25.5",
    "@types/babel-types": "^7.0.8",
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^9.0.10",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "execa": "^5.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "rollup": "^2.38.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.3.4",
    "sinon": "^9.2.4",
    "ts-mocha": "^8.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "react",
    "React CLI",
    "React CodeGen",
    "devkit",
    "sdk",
    "React DevKit",
    "code generator"
  ],
  "repository": "https://github.com/Cristians953/react-codegen-cli.git",
  "author": "Cristian Vasiliev <vasiliev.cristian99@gmail.com>",
  "license": "MIT",
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "readme",
              "release": "patch"
            },
            {
              "type": "refactor",
              "scope": "core-*",
              "release": "minor"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
